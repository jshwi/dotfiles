#!/bin/zsh
COMPLETION_WAITING_DOTS="true"
COMPLETION_WAITING_DOTS="true"
DISABLE_AUTO_TITLE="true"
DISABLE_AUTO_UPDATE="false"
DISABLE_LS_COLORS="false"
EDITOR="$VISUAL"
ENABLE_CORRECTION="true"
GEM="$HOME/.gem"
GEM_VER="2.5.0"
HISTFILE="$HOME/.zsh_history"
HISTSIZE='10000'
HIST_STAMPS="dd.mm.yyyy"
HYPHEN_INSENSITIVE="true"
LOCAL="$HOME/.local"
LOCAL_BIN="$LOCAL/bin"
NPM_BIN="$NPM_PACKAGES/bin"
NPM_PACKAGES="$HOME/.npm-packages"
RBENV="$HOME/.rbenv"
RBENV_BIN="$RBENV/bin"
RBENV_PLUGINS="$RBENV/plugins"
RUBYBUILD="$RBENV_PLUGINS/ruby-build"
RUBYBUILD_BIN="$RUBYBUILD/bin"
RUBY_GEM="$GEM/ruby"
RUBY_GEM_BIN="$RUBY_GEM/$GEM_VER/bin"
SAVEHIST="$HISTSIZE"
TERM="xterm-256color"
TRAVIS="$HOME/.travis"
TTC_GITBOT='gitlog'
TTC_SAY_BOX='parrot'
USR="usr"
USR_BIN="$USR/bin"
USR_LOCAL="$USR/local"
USR_LOCAL_BIN="$USR_LOCAL/bin"
VISUAL=vim
ZSHDDIR="$HOME/.zsh"
ZDOTDIR="$ZDOTDIR:-$HOME"
ZSH="$HOME/.oh-my-zsh"
ZSH_ALIASES="$ZSHDDIR/aliases"
ZSH_FUNCTIONS="$ZSHDDIR/functions"
ZSH_PLUGINS="$ZSHDDIR/plugins"
ZSHRC="$ZSHDDIR/.zshrc"
ZSH_SH="$ZSH/oh-my-zsh.sh"
ZSH_THEMES="$ZSHDDIR/themes"
ZSH_SYNTAX="$ZSHDDIR/syntax"
ZSH_PATHS="$ZSHDDIR/path"
ZSH_AUTOLOAD="$ZSHDDIR/autoload"
ZSH_EXPORTS="$ZSHDDIR/exports"
DIRCOLORS="$ZSHDDIR/dir_colors"
ZSH_ZSTYLE="$ZSHDDIR/zstyle"
